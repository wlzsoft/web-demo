<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.demo.dao.ImageResDao">
	
	<insert id="addImageRes" parameterType="com.pmp.entity.ImageResEntity" useGeneratedKeys="true" keyProperty="id">
			INSERT INTO imageres(
					md5Key,
					resourcesUrl
			)VALUES(
					#{md5Key},
					#{resourcesUrl}
			)
	</insert>
	
	
	
	<select id="getImageRes" parameterType="java.lang.Integer" resultType="com.pmp.entity.ImageResEntity" >
	    SELECT * FROM imageres a 
            WHERE a.md5Key=#{md5Key}
	</select>
	
</mapper>